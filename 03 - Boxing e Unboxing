    public class Program
    {
        
        public static void Main(string[] args)
        {
                       
           int a =10;
           object o = (object)a; //box
            
            Console.WriteLine(o.GetType().ToString());
            
            int b = (int)o; //unbox
            
            //int c = ((int)o).MaxValue;
            
            Console.WriteLine(b);
            Console.WriteLine(((int)o).ToString());
           
            
        }
    }
